            "Rating": null,
            "Ingredients": [
                "200g unsalted butter, softened",
                "100g caster sugar",
                "100g soft brown sugar",
                "1 large egg, beaten",
                "1 tsp vanilla extract",
                "350g plain flour, plus extra for dusting",
                "1 tsp baking powder",
                "100g dark chocolate chips, optional",
                "150g icing sugar, to decorate",
                "5 drops food colouring, to decorate",
                "1 sprinkles, to decorate",
                "Preparation time: 20 mins",
                "Cooking time: 10 mins",
                "Other time: 35 mins chilling/cooling"
